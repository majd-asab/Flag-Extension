{
  "manifest_version": 2,

  "name": "Flag Extention",
  "description": "flag of website",

   "content_scripts": [
    {
	"matches": ["*://*/*"],
        "js": ["extension-script.js"]

    }],
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon/flag.png"
  },
  "permissions": [
    "http://*/"
  ]
}
